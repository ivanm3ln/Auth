<Window x:Class="AuthProj.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AuthProj"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1200" ResizeMode="NoResize" Background="SeaShell">
    <Grid>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered"/>
        <Button x:Name="buttonBack" Content="Назад" FontSize="20" Margin="0,10,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="161" Height="61" Click="buttonBack_Click"/>
    </Grid>
</Window>
